{
  "name": "split-bill",
  "version": "0.1.0",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "name": "split-bill",
      "version": "0.1.0",
      "workspaces": [
        "apps/*",
        "packages/*",
        "infra/*"
      ],
      "engines": {
        "node": ">=20.0.0"
      }
    },
    "apps/backend": {
      "name": "@split-bill/backend",
      "version": "0.1.0"
    },
    "apps/frontend": {
      "name": "@split-bill/frontend",
      "version": "0.1.0"
    },
    "infra/devops": {
      "name": "@split-bill/devops",
      "version": "0.1.0"
    },
    "node_modules/@split-bill/backend": {
      "resolved": "apps/backend",
      "link": true
    },
    "node_modules/@split-bill/devops": {
      "resolved": "infra/devops",
      "link": true
    },
    "node_modules/@split-bill/frontend": {
      "resolved": "apps/frontend",
      "link": true
    }
  }
}
